{
  "name": "@quasar/quasar-ui-qflashcard",
  "version": "1.0.0",
  "author": "Jeff Galbraith <jeff@quasar.dev>",
  "description": "QFlashcard is an app extension for Quasar for making css mashups and transitions",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "main": "dist/index.common.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "dev": "cd dev && yarn dev && cd ..",
    "dev:umd": "yarn build && node build/script.open-umd.js",
    "dev:ssr": "cd dev && yarn 'dev:ssr' && cd ..",
    "dev:ios": "cd dev && yarn 'dev:ios' && cd ..",
    "dev:android": "cd dev && yarn 'dev:android' && cd ..",
    "dev:electron": "cd dev && yarn 'dev:electron' && cd ..",
    "build": "node build/index.js",
    "build:js": "node build/script.javascript.js",
    "build:css": "node build/script.css.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasarframework/quasar-ui-qflashcard.git"
  },
  "bugs": "https://github.com/quasarframework/quasar-ui-qflashcard/issues",
  "homepage": "https://github.com/quasarframework/quasar-ui-qflashcard",
  "keywords": [
    "quasar",
    "app",
    "app extension",
    "extension",
    "overlay"
  ],
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@vue/eslint-config-standard": "^5.0.1",
    "autoprefixer": "^9.7.3",
    "chalk": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-quasar": "^1.0.0-alpha.17",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.1",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.13.0",
    "open": "^7.0.0",
    "postcss": "^7.0.25",
    "postcss-rtl": "^1.5.0",
    "quasar": "^1.5.11",
    "quasar-json-api": "^0.0.16",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "uglify-es": "^3.3.9",
    "zlib": "^1.0.5"
